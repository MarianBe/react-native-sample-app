{
  "html": {
    "snippets": {
      "View": "<View style={[Styles.View]}>${1}</View>",
      "Text": "<Text style={[Styles.Text]}>${1}</Text>",
      "TouchableOpacity": "<TouchableOpacity style={[Styles.TouchableOpacity]} onPress={() => console.log('onpress')}>${1}</TouchableOpacity>",
      "FlatList": "<FlatList style={[Styles.FlatList]} data={[1, 2, 3]} renderItem={item => (<Text>{item}</Text>)}>${1}</FlatList>",
      "ScrollView": "<ScrollView style={[Styles.ScrollView]} contentContainerStyle={[Styles.ScrollViewContainer]}>${1}</ScrollView>",
      "I18": "{import I18n from 'react-native-i18n'}"
    }
  }
}
